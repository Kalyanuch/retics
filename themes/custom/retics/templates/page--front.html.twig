      <li data-filter=".residential">Residential</li>
      <li data-filter=".office">Office</li>
      <li data-filter=".commercial">Commercial</li>
    </ul>

    <div class="filters-content">
      <div class="row grid">
        <div class="col-sm-6 col-md-4 all residential">
          <div class="box">
            <img src="/{{ directory }}/images/p1.jpg" alt="">
          </div>
        </div>
